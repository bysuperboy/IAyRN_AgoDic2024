        "def es_primo(num):\n",
        "    if num < 2:\n",
        "        return False\n",
        "    for i in range(2, int(num ** 0.5) + 1):\n",
        "        if num % i == 0:\n",
        "            return False\n",
        "    return True\n",
        "\n",
        "def primos_en_rango(inicio, fin):\n",
        "    primos = []\n",
        "    for num in range(inicio, fin + 1):\n",
        "        if es_primo(num):\n",
        "            primos.append(num)\n",
        "    return primos\n",
        "\n",
        "# Solicitar el rango al usuario\n",
        "inicio = int(input(\"Ingresa el número de inicio del rango: \"))\n",
        "fin = int(input(\"Ingresa el número de fin del rango: \"))\n",
        "\n",
        "# Obtener los números primos en el rango y mostrarlos\n",
        "primos = primos_en_rango(inicio, fin)\n",
        "print(f\"Números primos en el rango de {inicio} a {fin}: {primos}\")\n",
        ""
      ]
